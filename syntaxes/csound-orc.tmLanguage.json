{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Csound ORC",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#strings"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [{
				"name": "keyword.control.csound-orc",
				"match": "\\b(if|else if|else|then|endif|goto|kgoto|igoto|while|until|do|od)\\b"
			}]
		},
		"strings": {
			"name": "string.quoted.double.csound-orc",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.csound-orc",
					"match": "\\\\."
				}
			]
		}
	},
	"scopeName": "source.csound.orc"
}